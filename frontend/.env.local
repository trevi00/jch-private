# Local Development Environment Variables
# This file is for local development only and should not be committed to git

# API Configuration
VITE_API_BASE_URL=http://localhost:8081/api
VITE_AI_SERVICE_BASE_URL=http://localhost:8001/api

# Application Configuration
VITE_APP_TITLE=잡았다 - 취업 준비 플랫폼 (개발환경)
VITE_APP_DESCRIPTION=취업 준비의 모든 것을 도와드리는 플랫폼
VITE_APP_VERSION=1.0.0-dev

# Development Features
VITE_ENABLE_DEBUG=true
VITE_ENABLE_DEV_TOOLS=true
VITE_ENABLE_ERROR_REPORTING=true

# Performance Settings
VITE_API_TIMEOUT=15000
VITE_AI_API_TIMEOUT=30000
VITE_RETRY_COUNT=3
VITE_CACHE_TIME=300000

# OAuth Configuration (development)
VITE_GOOGLE_CLIENT_ID=
VITE_OAUTH_REDIRECT_URL=http://localhost:3000/auth/callback

# Development Analytics (disabled)
VITE_ANALYTICS_ID=
